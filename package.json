{
  "name": "toi-microsite-Library",
  "version": "1.0.0",
  "description": "Component Library for TIL",
  "main": "",
  "browserslist": [
    ">1%"
  ],
  "scripts": {
    "start": "babel-node tools/run start",
    "storybook": "__PROD__=true storybook dev -p 6006",
    "build": "babel-node tools/run build",
    "storybook-build": "storybook build -o ./dist_storybook",
    "build-stats": "yarn run build --release --analyse",
    "lint-js": "eslint --quiet --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.scss\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js --fix",
    "fix-css": "yarn run lint-css --fix",
    "stop": "pkill -f http-server",
    "runstorybook": "rm -rf ./dist_storybook && yarn run storybook-build && http-server ./dist_storybook -g --cors -p 9001 --open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "lint-staged": {
    "*.js": [
      "eslint --no-ignore",
      "git add --force"
    ],
    "*.scss": [
      "stylelint",
      "git add --force"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add --force"
    ]
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "global": "^4.4.0",
    "http-server": "^14.1.1",
    "isomorphic-style-loader-react18": "^5.9.2",
    "lodash.find": "^4.6.0",
    "lodash.get": "^4.4.2",
    "lodash.throttle": "^4.1.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-slick": "^0.30.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-react-constant-elements": "^7.23.3",
    "@babel/plugin-transform-react-inline-elements": "^7.23.3",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@storybook/addon-actions": "^8.4.7",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/addon-themes": "^8.4.7",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@storybook/manager-api": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-webpack5": "^8.4.7",
    "@storybook/test": "^8.3.2",
    "@storybook/theming": "^8.4.7",
    "babel-loader": "^9.1.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-css-modules": "^2.12.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^8.0.3",
    "jsonp": "^0.2.1",
    "lint-staged": "^15.2.0",
    "node-sass": "^9.0.0",
    "prettier": "^3.1.1",
    "s3-deploy": "^1.4.0",
    "sass-loader": "^13.3.2",
    "storybook": "^8.4.7",
    "style-loader": "^3.3.3",
    "stylelint": "^16.0.2",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^35.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.0.0",
    "webpack": "^5.89.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "packageManager": "yarn@4.0.2"
}
